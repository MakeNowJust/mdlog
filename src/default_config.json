{
  "mdast_yaml": false,
  "mdast_breaks": true,

  "mdast_rule": "-",
  "mdast_ruleRepetition": 10,

  "stringify": true,

  "root_stringify": false,
  "root_block": true,

  "blockquote_block": true,
  "blockquote_italic": true,

  "heading_color": "red",
  "heading_style": "font-size: 1.42em",

  "code_color": "yellow",

  "inlineCode_underline": true,
  "inlineCode_style": "border: 1px solid #ccc; border-radius: 3px; padding: 1px; background: #f5f5f5;",

  "list_bold": true,

  "listItem_block": true,
  "listItem_bold": false,

  "emphasis_stringify": false,
  "emphasis_italic": true,

  "strong_stringify": false,
  "strong_bold": true,

  "delete_stringify": false,
  "delete_delete": true,

  "link_underline": true,
  "link_color": "magenta",

  "image_underline": true,
  "image_color": "cyan",

  "escape_stringify": false
}
